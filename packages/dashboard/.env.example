# Dashboard Configuration
# Copy this file to .env and fill in your values

# API Base URL — leave empty when using Vite proxy, or set to full URL
VITE_API_BASE_URL=http://localhost:3000

# Clerk Authentication (optional — auth disabled when not set)
# Must match the same Clerk application as the API package
VITE_CLERK_PUBLISHABLE_KEY=

# Error Retry (optional — default: 3)
VITE_MAX_RETRIES=3
